{
  "headerComment": "// Auto-generated barrel file\\n\\n",
  "targets": [
    "frontend/src",
    "backend/src"
  ],
  "forceGenerate": [
    "frontend/src/components/composite",
    "frontend/src/services",
    "backend/src/domain/ports"
  ],
  "exclude": [
    "**/__tests__/**",
    "**/components/ui/**",
    "**/*.test.*",
    "**/*.spec.*"
  ],
  "extensions": ["ts", "tsx"],
  "sortExports": true,
  "subdirectories": true,
  "verbose": false,
  "force": false
}